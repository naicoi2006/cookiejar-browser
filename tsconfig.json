{
	"compilerOptions": {
		"outDir": "dist",
		"target": "es2015",
		"lib": ["es2015", "es2016", "es2017", "es2018"],
		"module": "commonjs",
		"moduleResolution": "node",
		"declaration": true,
		"pretty": true,
		"newLine": "lf",
		"stripInternal": true,
		"strict": false,
		"sourceMap": true,
		"esModuleInterop": true
	},
	"exclude": ["node_modules", "dist"]
}
